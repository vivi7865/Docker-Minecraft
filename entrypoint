cd server
if [ -a /home/minecraft/startup.sh ]
  if [ -x /home/minecraft/startup.sh ]
    ../startup.sh
    exit 1
  else
    echo "startup.sh is not executable"
  end
end

if [ -a /home/minecraft/server.jar ]
  java $* -jar ../server.jar
else
  echo "server.jar doesn't exist !"
  echo "exiting"
  exit 1
end
